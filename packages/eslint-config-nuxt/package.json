{
  "name": "@hedgehoglab/eslint-config-nuxt",
  "version": "2.0.0",
  "description": "Nuxt eslint config rules for the hedgehog lab org",
  "keywords": [
    "eslint-config",
    "eslint",
    "hedgehoglab",
    "nuxt"
  ],
  "homepage": "https://github.com/hedgehoglab-engineering/frontend-linters/tree/main/packages/eslint-config-nuxt",
  "license": "ISC",
  "author": "Sam Turrell <sam.turrell@hedgehoglab.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "test": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.1.1",
    "typescript": "^5.5.3"
  }
}
